{
  "apps": [
    {
      "key": "current-user",
      "load": {
        "remote": {
          "key": "current_user",
          "entry": "./assets/apps/user/current-user/remoteEntry.js",
          "exposedModule": "./app",
          "development": {
            "entry": "http://localhost:4205/remoteEntry.js",
            "active": false
          }
        }
      },
      "handle": {
        "type": "angular-component",
        "component": "AppComponent",
        "module": "AppModule"
      }
    }
  ],
  "extension-points": [{ "key": "current-user", "app": "current-user" }]
}
