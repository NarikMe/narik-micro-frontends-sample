{
  "name": "narik-micro-frontends",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:shared": "ng build shopping-lib",
    "build:apps": "ng build shell && npm run build:shopping-cart && npm run build:user && npm run build:products && npm run build:tax-service && npm run build:vue-apps",
    "build:vue-apps": "npm run build --prefix apps/payment-type1 && npm run build --prefix apps/payment-type2",
    "build:shopping-cart": "ng build shopping-cart  && ng build shopping-cart-button && ng build shopping-cart-service ",
    "build:user": "ng build user-profile && ng build current-user ",
    "build:products": "ng build products",
    "build:tax-service": "ng build tax-service"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.0",
    "@angular/cdk": "~14.0.0",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.0",
    "@angular/core": "~14.0.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~14.0.0",
    "@angular/material": "14.0.0",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@narik/micro-frontends-core": "^2.0.1",
    "@narik/micro-frontends-infrastructure": "^2.0.1",
    "@narik/micro-frontends-ui": "^2.0.1",
    "@narik/micro-frontends-vue-handlers": "^2.0.0",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^14.2.3",
    "@angular-builders/custom-webpack": "~13.1.0",
    "@angular-devkit/build-angular": "~14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "~14.0.0",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^17.0.39",
    "jasmine-core": "~4.1.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.0.0",
    "typescript": "~4.7.3"
  }
}
