{
  "name": "narik-micro-frontends",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:shared": "ng build shopping-lib",
    "build:apps": "ng build shell && npm run build:shopping-cart && npm run build:user && npm run build:products && npm run build:tax-service && npm run build:vue-apps",
    "build:vue-apps": "npm run build --prefix apps/payment-type1 && npm run build --prefix apps/payment-type2",
    "build:shopping-cart": "ng build shopping-cart  && ng build shopping-cart-button && ng build shopping-cart-service ",
    "build:user": "ng build user-profile && ng build current-user ",
    "build:products": "ng build products",
    "build:tax-service": "ng build tax-service"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "~13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.1.1",
    "@angular/material": "13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@narik/micro-frontends-core": "^2.0.1",
    "@narik/micro-frontends-infrastructure": "^2.0.1",
    "@narik/micro-frontends-ui": "^2.0.1",
    "@narik/micro-frontends-vue-handlers": "^2.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^14.0.1",
    "@angular-builders/custom-webpack": "~13.0.0",
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@types/jasmine": "~3.10.2",
    "@types/node": "^17.0.4",
    "jasmine-core": "~3.10.1",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.1.2",
    "typescript": "~4.5.4"
  }
