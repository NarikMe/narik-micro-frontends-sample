{
  "name": "narik-micro-frontends",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:shared": "ng build shopping-lib",
    "build:apps": "ng build shell && npm run build:shopping-cart && npm run build:user && npm run build:products && npm run build:tax-service && npm run build:vue-apps",
    "build:vue-apps": "npm run build --prefix apps/payment-type1 && npm run build --prefix apps/payment-type2",
    "build:shopping-cart": "ng build shopping-cart  && ng build shopping-cart-button && ng build shopping-cart-service ",
    "build:user": "ng build user-profile && ng build current-user ",
    "build:products": "ng build products",
    "build:tax-service": "ng build tax-service"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "12.2.1",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.2.0",
    "@angular/material": "12.2.1",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@narik/micro-frontends-core": "^0.0.1",
    "@narik/micro-frontends-infrastructure": "^0.0.1",
    "@narik/micro-frontends-ui": "^0.0.1",
    "rxjs": "~7.3.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^12.4.0",
    "@angular-builders/custom-webpack": "~12.1.0",
    "@angular-devkit/build-angular": "~12.2.1",
    "@angular/cli": "~12.2.1",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "typescript": "~4.3.5"
  }
}
